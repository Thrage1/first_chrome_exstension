{
  "manifest_version": 2,
  "name": "AutoOpen tab",
  "version": "1.0",
  "browser_action": {
      "default_popup": "tab.html",
      "default_icon": "img.png"
    },
  "commands": {
  "_execute_browser_action": {
    "suggested_key": {
      "default": "Ctrl+Z",
      "mac": "MacCtrl+Z"
    },
    "description": "Opens all urls"
    }
  }

}
